[
    {
        "image": "nginx",
        "name": "webserver1",
        "network": "webservers",
        "environment": {
            "NGINX_HOST": "foobar.com"
        }
    },
    {
        "image": "postgres",
        "name": "db1",
        "volumes": {
            "/tmp/db1/": {
                "bind": "/var/lib/postgresql/data",
                "mode": "rw"
            }
        }
    },
    {
        "image": "redis",
        "name": "cache1"
    }
]